<!-- Birthday Sunshine Bouquet -->
<svg width="400" height="500" viewBox="0 0 400 500" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <radialGradient id="sunshine" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#ffd700;stop-opacity:1" />
      <stop offset="70%" style="stop-color:#ffa500;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ff8c00;stop-opacity:1" />
    </radialGradient>
    <radialGradient id="orange" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#ff7f50;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ff6347;stop-opacity:1" />
    </radialGradient>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="500" fill="#fffacd"/>
  
  <!-- Cheerful Wrapper -->
  <path d="M 150 350 Q 200 380 250 350 L 250 450 Q 200 470 150 450 Z" fill="#daa520" opacity="0.8"/>
  
  <!-- Stems -->
  <rect x="195" y="300" width="6" height="120" fill="#9acd32"/>
  <rect x="185" y="310" width="5" height="110" fill="#9acd32"/>
  <rect x="205" y="305" width="5" height="115" fill="#9acd32"/>
  <rect x="175" y="320" width="4" height="100" fill="#9acd32"/>
  <rect x="215" y="315" width="4" height="105" fill="#9acd32"/>
  
  <!-- Sunshine Flowers -->
  <g transform="translate(200,200)">
    <!-- Center - Sunflower -->
    <circle cx="0" cy="0" r="35" fill="url(#sunshine)"/>
    <circle cx="0" cy="0" r="25" fill="#ffd700"/>
    <circle cx="0" cy="0" r="15" fill="#8b4513" opacity="0.8"/>
    
    <!-- Sunflower petals -->
    <g transform="translate(0,0)">
      <ellipse cx="0" cy="-30" rx="8" ry="15" fill="#ffd700" transform="rotate(0)"/>
      <ellipse cx="0" cy="-30" rx="8" ry="15" fill="#ffd700" transform="rotate(30)"/>
      <ellipse cx="0" cy="-30" rx="8" ry="15" fill="#ffd700" transform="rotate(60)"/>
      <ellipse cx="0" cy="-30" rx="8" ry="15" fill="#ffd700" transform="rotate(90)"/>
      <ellipse cx="0" cy="-30" rx="8" ry="15" fill="#ffd700" transform="rotate(120)"/>
      <ellipse cx="0" cy="-30" rx="8" ry="15" fill="#ffd700" transform="rotate(150)"/>
      <ellipse cx="0" cy="-30" rx="8" ry="15" fill="#ffd700" transform="rotate(180)"/>
      <ellipse cx="0" cy="-30" rx="8" ry="15" fill="#ffd700" transform="rotate(210)"/>
      <ellipse cx="0" cy="-30" rx="8" ry="15" fill="#ffd700" transform="rotate(240)"/>
      <ellipse cx="0" cy="-30" rx="8" ry="15" fill="#ffd700" transform="rotate(270)"/>
      <ellipse cx="0" cy="-30" rx="8" ry="15" fill="#ffd700" transform="rotate(300)"/>
      <ellipse cx="0" cy="-30" rx="8" ry="15" fill="#ffd700" transform="rotate(330)"/>
    </g>
    
    <!-- Orange Gerberas -->
    <circle cx="-45" cy="25" r="32" fill="url(#orange)"/>
    <circle cx="50" cy="20" r="30" fill="#ff7f50"/>
    <circle cx="30" cy="-35" r="33" fill="url(#orange)"/>
    <circle cx="0" cy="60" r="29" fill="#ff6347"/>
    
    <!-- Yellow Marigolds -->
    <circle cx="-25" cy="-40" r="28" fill="#ffd700"/>
    <circle cx="65" cy="-10" r="27" fill="#ffb347"/>
    <circle cx="-60" cy="-15" r="25" fill="#ffd700"/>
    
    <!-- Inner details -->
    <circle cx="-45" cy="25" r="15" fill="#ff4500" opacity="0.7"/>
    <circle cx="50" cy="20" r="13" fill="#ff4500" opacity="0.7"/>
    <circle cx="30" cy="-35" r="16" fill="#ff4500" opacity="0.7"/>
  </g>
  
  <!-- Bright Green Leaves -->
  <ellipse cx="140" cy="250" rx="15" ry="25" fill="#9acd32" transform="rotate(-20)"/>
  <ellipse cx="260" cy="245" rx="12" ry="22" fill="#32cd32" transform="rotate(25)"/>
  <ellipse cx="170" cy="280" rx="10" ry="18" fill="#9acd32" transform="rotate(-45)"/>
  <ellipse cx="230" cy="275" rx="11" ry="19" fill="#32cd32" transform="rotate(30)"/>
  
  <!-- Bright Ribbon -->
  <ellipse cx="200" cy="350" rx="45" ry="10" fill="#ff6347"/>
  <path d="M 155 350 Q 145 340 135 350 Q 145 360 155 350" fill="#ff6347"/>
  <path d="M 245 350 Q 255 340 265 350 Q 255 360 245 350" fill="#ff6347"/>
  
  <!-- Celebration sparkles -->
  <circle cx="120" cy="150" r="3" fill="#ffd700" opacity="0.8"/>
  <circle cx="280" cy="180" r="2" fill="#ff6347" opacity="0.8"/>
  <circle cx="350" cy="250" r="3" fill="#ffd700" opacity="0.8"/>
  <circle cx="50" cy="300" r="2" fill="#ff7f50" opacity="0.8"/>
</svg>