<!-- Rainbow Mix Bouquet -->
<svg width="400" height="500" viewBox="0 0 400 500" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <radialGradient id="rainbow1" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#ff6b6b;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ee5a52;stop-opacity:1" />
    </radialGradient>
    <radialGradient id="rainbow2" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#feca57;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ff9ff3;stop-opacity:1" />
    </radialGradient>
    <radialGradient id="rainbow3" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#48dbfb;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0abde3;stop-opacity:1" />
    </radialGradient>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="500" fill="#f1f2f6"/>
  
  <!-- Bouquet Wrapper -->
  <path d="M 150 350 Q 200 380 250 350 L 250 450 Q 200 470 150 450 Z" fill="#8b4513" opacity="0.8"/>
  
  <!-- Stems -->
  <rect x="195" y="300" width="6" height="120" fill="#6ab04c"/>
  <rect x="185" y="310" width="5" height="110" fill="#6ab04c"/>
  <rect x="205" y="305" width="5" height="115" fill="#6ab04c"/>
  <rect x="175" y="320" width="4" height="100" fill="#6ab04c"/>
  <rect x="215" y="315" width="4" height="105" fill="#6ab04c"/>
  
  <!-- Rainbow Mix Flowers -->
  <g transform="translate(200,200)">
    <!-- Center - Yellow Rose -->
    <circle cx="0" cy="0" r="35" fill="url(#rainbow2)"/>
    <circle cx="0" cy="0" r="25" fill="#feca57" opacity="0.8"/>
    
    <!-- Red Roses -->
    <circle cx="-45" cy="25" r="32" fill="url(#rainbow1)"/>
    <circle cx="30" cy="-35" r="33" fill="#ff4757"/>
    
    <!-- Pink Roses -->
    <circle cx="50" cy="20" r="30" fill="#ff9ff3"/>
    <circle cx="0" cy="60" r="29" fill="#fd79a8"/>
    
    <!-- Blue/Purple Flowers -->
    <circle cx="-25" cy="-40" r="28" fill="url(#rainbow3)"/>
    <circle cx="65" cy="-10" r="27" fill="#a29bfe"/>
    
    <!-- Orange Flowers -->
    <circle cx="-60" cy="-15" r="25" fill="#ff7675"/>
    
    <!-- White Lilies -->
    <g transform="translate(-70, 40)">
      <ellipse cx="0" cy="0" rx="15" ry="8" fill="#fff" transform="rotate(0)"/>
      <ellipse cx="0" cy="0" rx="15" ry="8" fill="#fff" transform="rotate(60)"/>
      <ellipse cx="0" cy="0" rx="15" ry="8" fill="#fff" transform="rotate(120)"/>
    </g>
    
    <g transform="translate(75, 35)">
      <ellipse cx="0" cy="0" rx="12" ry="6" fill="#fff" transform="rotate(0)"/>
      <ellipse cx="0" cy="0" rx="12" ry="6" fill="#fff" transform="rotate(60)"/>
      <ellipse cx="0" cy="0" rx="12" ry="6" fill="#fff" transform="rotate(120)"/>
    </g>
  </g>
  
  <!-- Colorful Leaves -->
  <ellipse cx="140" cy="250" rx="15" ry="25" fill="#00b894" transform="rotate(-20)"/>
  <ellipse cx="260" cy="245" rx="12" ry="22" fill="#55a3ff" transform="rotate(25)"/>
  <ellipse cx="170" cy="280" rx="10" ry="18" fill="#00b894" transform="rotate(-45)"/>
  <ellipse cx="230" cy="275" rx="11" ry="19" fill="#6c5ce7" transform="rotate(30)"/>
  
  <!-- Rainbow Ribbon -->
  <ellipse cx="200" cy="350" rx="45" ry="10" fill="#ff6b6b" opacity="0.8"/>
  <ellipse cx="200" cy="352" rx="45" ry="8" fill="#feca57" opacity="0.6"/>
  <ellipse cx="200" cy="354" rx="45" ry="6" fill="#48dbfb" opacity="0.4"/>
</svg>