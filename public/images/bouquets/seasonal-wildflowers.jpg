<!-- Seasonal Wildflowers Bouquet -->
<svg width="400" height="500" viewBox="0 0 400 500" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <radialGradient id="wildflower1" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#dda0dd;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#9370db;stop-opacity:1" />
    </radialGradient>
    <radialGradient id="wildflower2" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#87ceeb;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#4682b4;stop-opacity:1" />
    </radialGradient>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="500" fill="#f0f8ff"/>
  
  <!-- Rustic Wrapper -->
  <path d="M 150 350 Q 200 380 250 350 L 250 450 Q 200 470 150 450 Z" fill="#8fbc8f" opacity="0.7"/>
  <rect x="190" y="350" width="20" height="120" fill="#deb887" opacity="0.6"/>
  
  <!-- Natural Stems -->
  <rect x="195" y="300" width="4" height="120" fill="#6b8e23"/>
  <rect x="185" y="310" width="3" height="110" fill="#556b2f"/>
  <rect x="205" y="305" width="4" height="115" fill="#6b8e23"/>
  <rect x="175" y="320" width="3" height="100" fill="#556b2f"/>
  <rect x="215" y="315" width="3" height="105" fill="#6b8e23"/>
  <rect x="225" y="325" width="3" height="95" fill="#556b2f"/>
  
  <!-- Wildflowers -->
  <g transform="translate(200,200)">
    <!-- Purple Wildflowers -->
    <circle cx="-30" cy="-20" r="15" fill="url(#wildflower1)"/>
    <circle cx="40" cy="-25" r="12" fill="#dda0dd"/>
    <circle cx="-50" cy="10" r="18" fill="#9370db"/>
    
    <!-- Blue Wildflowers -->
    <circle cx="25" cy="15" r="14" fill="url(#wildflower2)"/>
    <circle cx="-15" cy="35" r="16" fill="#87ceeb"/>
    <circle cx="55" cy="30" r="13" fill="#4682b4"/>
    
    <!-- White Daisies -->
    <g transform="translate(0,-40)">
      <circle cx="0" cy="0" r="8" fill="#fff"/>
      <ellipse cx="0" cy="-12" rx="3" ry="8" fill="#fff" transform="rotate(0)"/>
      <ellipse cx="0" cy="-12" rx="3" ry="8" fill="#fff" transform="rotate(30)"/>
      <ellipse cx="0" cy="-12" rx="3" ry="8" fill="#fff" transform="rotate(60)"/>
      <ellipse cx="0" cy="-12" rx="3" ry="8" fill="#fff" transform="rotate(90)"/>
      <ellipse cx="0" cy="-12" rx="3" ry="8" fill="#fff" transform="rotate(120)"/>
      <ellipse cx="0" cy="-12" rx="3" ry="8" fill="#fff" transform="rotate(150)"/>
      <circle cx="0" cy="0" r="3" fill="#ffd700"/>
    </g>
    
    <g transform="translate(-60,-30)">
      <circle cx="0" cy="0" r="6" fill="#fff"/>
      <ellipse cx="0" cy="-9" rx="2" ry="6" fill="#fff" transform="rotate(0)"/>
      <ellipse cx="0" cy="-9" rx="2" ry="6" fill="#fff" transform="rotate(45)"/>
      <ellipse cx="0" cy="-9" rx="2" ry="6" fill="#fff" transform="rotate(90)"/>
      <ellipse cx="0" cy="-9" rx="2" ry="6" fill="#fff" transform="rotate(135)"/>
      <circle cx="0" cy="0" r="2" fill="#ffd700"/>
    </g>
    
    <!-- Small scattered flowers -->
    <circle cx="70" cy="-15" r="6" fill="#ff69b4"/>
    <circle cx="-70" cy="40" r="5" fill="#98fb98"/>
    <circle cx="35" cy="50" r="7" fill="#ffa07a"/>
    <circle cx="-35" cy="60" r="6" fill="#dda0dd"/>
    
    <!-- Grass/wheat elements -->
    <ellipse cx="-80" cy="20" rx="2" ry="15" fill="#9acd32" transform="rotate(-15)"/>
    <ellipse cx="80" cy="10" rx="2" ry="12" fill="#9acd32" transform="rotate(20)"/>
    <ellipse cx="-45" cy="70" rx="1.5" ry="10" fill="#9acd32" transform="rotate(-10)"/>
    <ellipse cx="60" cy="65" rx="1.5" ry="8" fill="#9acd32" transform="rotate(15)"/>
  </g>
  
  <!-- Natural Leaves and Foliage -->
  <ellipse cx="130" cy="250" rx="12" ry="20" fill="#6b8e23" transform="rotate(-25)"/>
  <ellipse cx="270" cy="245" rx="10" ry="18" fill="#556b2f" transform="rotate(30)"/>
  <ellipse cx="160" cy="290" rx="8" ry="15" fill="#9acd32" transform="rotate(-50)"/>
  <ellipse cx="240" cy="285" rx="9" ry="16" fill="#6b8e23" transform="rotate(35)"/>
  <ellipse cx="200" cy="320" rx="6" ry="12" fill="#556b2f" transform="rotate(5)"/>
  
  <!-- Twine/Natural Binding -->
  <ellipse cx="200" cy="350" rx="35" ry="8" fill="#deb887"/>
  <path d="M 165 350 Q 155 345 150 350 Q 155 355 165 350" fill="#deb887"/>
  <path d="M 235 350 Q 245 345 250 350 Q 245 355 235 350" fill="#deb887"/>
  
  <!-- Natural texture lines -->
  <line x1="190" y1="345" x2="210" y2="355" stroke="#8b7355" stroke-width="1" opacity="0.6"/>
  <line x1="185" y1="350" x2="215" y2="350" stroke="#8b7355" stroke-width="1" opacity="0.6"/>
</svg>