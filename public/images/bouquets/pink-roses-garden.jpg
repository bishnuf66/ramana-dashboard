<!-- Garden Pink Roses Bouquet -->
<svg width="400" height="500" viewBox="0 0 400 500" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <radialGradient id="pinkRose" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#ff9ff3;stop-opacity:1" />
      <stop offset="70%" style="stop-color:#f368e0;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e84393;stop-opacity:1" />
    </radialGradient>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="500" fill="#fef7f0"/>
  
  <!-- Bouquet Wrapper -->
  <path d="M 150 350 Q 200 380 250 350 L 250 450 Q 200 470 150 450 Z" fill="#d4a574" opacity="0.9"/>
  
  <!-- Stems -->
  <rect x="195" y="300" width="6" height="120" fill="#6ab04c"/>
  <rect x="185" y="310" width="5" height="110" fill="#6ab04c"/>
  <rect x="205" y="305" width="5" height="115" fill="#6ab04c"/>
  <rect x="175" y="320" width="4" height="100" fill="#6ab04c"/>
  <rect x="215" y="315" width="4" height="105" fill="#6ab04c"/>
  
  <!-- Pink Roses -->
  <g transform="translate(200,200)">
    <!-- Center Rose -->
    <circle cx="0" cy="0" r="35" fill="url(#pinkRose)"/>
    <circle cx="0" cy="0" r="25" fill="#fd79a8" opacity="0.8"/>
    <circle cx="0" cy="0" r="15" fill="#fdcb6e" opacity="0.3"/>
    
    <!-- Surrounding Roses -->
    <circle cx="-45" cy="25" r="32" fill="url(#pinkRose)"/>
    <circle cx="50" cy="20" r="30" fill="#ff7675"/>
    <circle cx="-25" cy="-40" r="28" fill="url(#pinkRose)"/>
    <circle cx="30" cy="-35" r="33" fill="#fd79a8"/>
    <circle cx="-60" cy="-15" r="25" fill="#ff7675"/>
    <circle cx="65" cy="-10" r="27" fill="url(#pinkRose)"/>
    <circle cx="0" cy="60" r="29" fill="#fd79a8"/>
    
    <!-- Baby's breath (small white flowers) -->
    <circle cx="-70" cy="40" r="4" fill="#fff" opacity="0.9"/>
    <circle cx="75" cy="35" r="3" fill="#fff" opacity="0.9"/>
    <circle cx="-35" cy="70" r="4" fill="#fff" opacity="0.9"/>
    <circle cx="40" cy="65" r="3" fill="#fff" opacity="0.9"/>
    <circle cx="-80" cy="-30" r="3" fill="#fff" opacity="0.9"/>
    <circle cx="80" cy="-25" r="4" fill="#fff" opacity="0.9"/>
  </g>
  
  <!-- Garden Leaves -->
  <ellipse cx="140" cy="250" rx="15" ry="25" fill="#00b894" transform="rotate(-20)"/>
  <ellipse cx="260" cy="245" rx="12" ry="22" fill="#00b894" transform="rotate(25)"/>
  <ellipse cx="170" cy="280" rx="10" ry="18" fill="#55a3ff" transform="rotate(-45)"/>
  <ellipse cx="230" cy="275" rx="11" ry="19" fill="#00b894" transform="rotate(30)"/>
  <ellipse cx="200" cy="320" rx="8" ry="14" fill="#55a3ff" transform="rotate(10)"/>
  
  <!-- Soft Pink Ribbon -->
  <ellipse cx="200" cy="350" rx="45" ry="10" fill="#ff9ff3" opacity="0.8"/>
  <path d="M 155 350 Q 145 340 135 350 Q 145 360 155 350" fill="#ff9ff3"/>
  <path d="M 245 350 Q 255 340 265 350 Q 255 360 245 350" fill="#ff9ff3"/>
</svg>