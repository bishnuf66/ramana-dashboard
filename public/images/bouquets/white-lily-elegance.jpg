<!-- White Lily Elegance Bouquet -->
<svg width="400" height="500" viewBox="0 0 400 500" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <radialGradient id="lily" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#fff;stop-opacity:1" />
      <stop offset="80%" style="stop-color:#f8f9fa;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e9ecef;stop-opacity:1" />
    </radialGradient>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="500" fill="#f8f9fa"/>
  
  <!-- Elegant Wrapper -->
  <path d="M 150 350 Q 200 380 250 350 L 250 450 Q 200 470 150 450 Z" fill="#495057" opacity="0.8"/>
  
  <!-- Stems -->
  <rect x="195" y="300" width="6" height="120" fill="#28a745"/>
  <rect x="185" y="310" width="5" height="110" fill="#28a745"/>
  <rect x="205" y="305" width="5" height="115" fill="#28a745"/>
  
  <!-- White Lilies -->
  <g transform="translate(200,200)">
    <!-- Center Lily -->
    <g transform="translate(0,0)">
      <ellipse cx="0" cy="0" rx="25" ry="12" fill="url(#lily)" transform="rotate(0)"/>
      <ellipse cx="0" cy="0" rx="25" ry="12" fill="url(#lily)" transform="rotate(60)"/>
      <ellipse cx="0" cy="0" rx="25" ry="12" fill="url(#lily)" transform="rotate(120)"/>
      <ellipse cx="0" cy="0" rx="25" ry="12" fill="url(#lily)" transform="rotate(180)"/>
      <ellipse cx="0" cy="0" rx="25" ry="12" fill="url(#lily)" transform="rotate(240)"/>
      <ellipse cx="0" cy="0" rx="25" ry="12" fill="url(#lily)" transform="rotate(300)"/>
      <!-- Stamen -->
      <circle cx="0" cy="0" r="3" fill="#ffd700"/>
      <circle cx="2" cy="1" r="1" fill="#ff6b35"/>
      <circle cx="-2" cy="1" r="1" fill="#ff6b35"/>
      <circle cx="0" cy="-2" r="1" fill="#ff6b35"/>
    </g>
    
    <!-- Side Lilies -->
    <g transform="translate(-50,30)">
      <ellipse cx="0" cy="0" rx="20" ry="10" fill="url(#lily)" transform="rotate(0)"/>
      <ellipse cx="0" cy="0" rx="20" ry="10" fill="url(#lily)" transform="rotate(60)"/>
      <ellipse cx="0" cy="0" rx="20" ry="10" fill="url(#lily)" transform="rotate(120)"/>
      <circle cx="0" cy="0" r="2" fill="#ffd700"/>
    </g>
    
    <g transform="translate(55,25)">
      <ellipse cx="0" cy="0" rx="22" ry="11" fill="url(#lily)" transform="rotate(0)"/>
      <ellipse cx="0" cy="0" rx="22" ry="11" fill="url(#lily)" transform="rotate(60)"/>
      <ellipse cx="0" cy="0" rx="22" ry="11" fill="url(#lily)" transform="rotate(120)"/>
      <circle cx="0" cy="0" r="2" fill="#ffd700"/>
    </g>
    
    <g transform="translate(-30,-45)">
      <ellipse cx="0" cy="0" rx="18" ry="9" fill="url(#lily)" transform="rotate(0)"/>
      <ellipse cx="0" cy="0" rx="18" ry="9" fill="url(#lily)" transform="rotate(60)"/>
      <ellipse cx="0" cy="0" rx="18" ry="9" fill="url(#lily)" transform="rotate(120)"/>
      <circle cx="0" cy="0" r="2" fill="#ffd700"/>
    </g>
    
    <g transform="translate(40,-40)">
      <ellipse cx="0" cy="0" rx="21" ry="10" fill="url(#lily)" transform="rotate(0)"/>
      <ellipse cx="0" cy="0" rx="21" ry="10" fill="url(#lily)" transform="rotate(60)"/>
      <ellipse cx="0" cy="0" rx="21" ry="10" fill="url(#lily)" transform="rotate(120)"/>
      <circle cx="0" cy="0" r="2" fill="#ffd700"/>
    </g>
    
    <!-- Small accent lilies -->
    <g transform="translate(-70,-5)">
      <ellipse cx="0" cy="0" rx="15" ry="7" fill="url(#lily)" transform="rotate(0)"/>
      <ellipse cx="0" cy="0" rx="15" ry="7" fill="url(#lily)" transform="rotate(60)"/>
      <ellipse cx="0" cy="0" rx="15" ry="7" fill="url(#lily)" transform="rotate(120)"/>
    </g>
    
    <g transform="translate(70,5)">
      <ellipse cx="0" cy="0" rx="16" ry="8" fill="url(#lily)" transform="rotate(0)"/>
      <ellipse cx="0" cy="0" rx="16" ry="8" fill="url(#lily)" transform="rotate(60)"/>
      <ellipse cx="0" cy="0" rx="16" ry="8" fill="url(#lily)" transform="rotate(120)"/>
    </g>
  </g>
  
  <!-- Elegant Green Leaves -->
  <ellipse cx="140" cy="250" rx="18" ry="30" fill="#28a745" transform="rotate(-20)"/>
  <ellipse cx="260" cy="245" rx="15" ry="25" fill="#28a745" transform="rotate(25)"/>
  <ellipse cx="170" cy="290" rx="12" ry="20" fill="#20c997" transform="rotate(-45)"/>
  <ellipse cx="230" cy="285" rx="14" ry="22" fill="#28a745" transform="rotate(30)"/>
  
  <!-- Silver Ribbon -->
  <ellipse cx="200" cy="350" rx="50" ry="12" fill="#6c757d" opacity="0.8"/>
  <path d="M 150 350 Q 140 340 130 350 Q 140 360 150 350" fill="#6c757d"/>
  <path d="M 250 350 Q 260 340 270 350 Q 260 360 250 350" fill="#6c757d"/>
</svg>